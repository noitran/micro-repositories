{
    "name": "noitran/micro-repositories",
    "description": "Repositories package for laravel / lumen projects",
    "authors": [
        {
            "name": "Noitran.Black",
            "email": "noitran.black@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "lumen",
        "api",
        "repositories",
        "eloquent",
        "model",
        "mysql",
        "mongodb",
        "microservice"
    ],
    "license": "LGPL-3.0",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.4.2|~3.5.0|~3.6.0|~3.7.0",
        "nesbot/carbon": "^1.26.3"
    },
    "autoload": {
        "psr-4": {
            "Noitran\\Repositories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noitran\\Repositories\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noitran\\Repositories\\ServiceProvider"
            ]
        }
    }
}
