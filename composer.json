{
    "name": "noitran/micro-repositories",
    "description": "Repositories package for laravel / lumen projects",
    "authors": [
        {
            "name": "Noitran.Black",
            "email": "noitran.black@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "lumen",
        "api",
        "repositories",
        "eloquent",
        "model",
        "mysql",
        "mongodb",
        "microservice"
    ],
    "license": "LGPL-3.0",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^5.8|^6.0",
        "noitran/rql": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.3",
        "orchestra/testbench": "~3.8.0",
        "nesbot/carbon": "^2.23.0"
    },
    "autoload": {
        "psr-4": {
            "Noitran\\Repositories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noitran\\Repositories\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noitran\\Repositories\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

